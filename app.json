{
    "name": "painkilibot",
    "logo": "https://telegra.ph/file/62f59f408140d4972c7c1.jpg",
    "description": "Open-Source bot to play songs in your Telegram's Group Voice Chat. Powered by PyTgCalls.",
    "keywords": ["music", "voicechat", "telegram"],
    "repository": "https://github.com/sakhaavvaavaj93/painkilibot",
    "stack": "container",
    "env": {
        "SESSION_NAME": {
            "description": "use @PyroStringSession_bot ",
            "required": true
        },
        "BOT_TOKEN": {
            "description": "From @BotFather",
            "required": true
        },
        "BOT_NAME": {
            "description": "Your VCPlayBot Name.",
            "required": false,
            "value": ""
       },
        "API_ID": {
            "description": "From my.telegram.org/apps",
            "required": true
        },
        "API_HASH": {
            "description": "From my.telegram.org/apps",
            "required": true
        },
        "SUDO_USERS": {
            "description": "Full-access users, counted as admin everywhere, seperated by space.",
            "required": true
        },
        "DURATION_LIMIT": {
            "description": "In minutes!",
            "required": true,
            "value": "30"
        }
    },
    "buildpacks": [
       {
      "url": "heroku/python"
       }
     ]
  }
